{
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended",
    "next/core-web-vitals"
  ],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": false
      }
    ],
    "no-unused-vars": "warn",
    "prefer-destructuring": "off",
    "no-unused-expressions": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
